pipeline {
    agent any

    stages{
        stage('Buildear'){
            steps{
                sh 'echo Buildeando la aplicacion'
            }
        }
        stage('Testear'){
            steps{
                sh 'echo Testeando la aplicacion'
            }
        }
        stage('Deployar'){
            steps{
                sh 'echo Deployando la aplicacion'
            }
        }
    }
}